.scroll-x {
  display: flex;
  flex-flow: row wrap;
  align-content: flex-start;
  background: #fff;
  padding: 15rpx 10rpx 10rpx;
  /* margin: 0 30rpx 40rpx; */
  margin: 0 30rpx 10rpx;
  overflow: hidden;
  /* height: 85px; */
  height: 170px;
  width: 672rpx;
  /* 滑块时需要加上下面属性 */
  /* white-space: nowrap; */
}

.main-core {
  display: flex;
  flex-flow: row wrap;
  align-content: flex-start;
  background: #fff;
  border-radius: 10rpx;
  border-bottom: 1rpx solid #e5e5e5;
  box-shadow: 5rpx 5rpx 15rpx #ccc;
  padding: 15rpx 10rpx 10rpx;
  margin: 30rpx 30rpx 20rpx;
  overflow: hidden;
}

.main-core-item {
  display: inline-block;
  flex-flow: column wrap;
  justify-content: center;
  align-items: center;
  box-sizing: border-box;
  /* width: 22.7%; */
  width: 25%;
  height: 170rpx;
}

.core-item-icon {
  display: block;
  width: 100rpx;
  height: 100rpx;
  margin: 10rpx auto;
}

.core-item-name {
  display: block;
  margin: 5rpx;
  font-size: 28rpx;
  text-align: center;
}

.main-card {
  padding-bottom: 50rpx;
}

.main-card-item {
  display: flex;
  flex-direction: column;
  background: #fff;
  border-top: 1rpx solid #e5e5e5;
  border-bottom: 1rpx solid #e5e5e5;
  border-radius: 10rpx;
  box-shadow: 5rpx 5rpx 15rpx #ccc;
  margin: 0 30rpx 30rpx;
  background-repeat: no-repeat;
  background-size: 100% auto;
  background-position: bottom center;
  overflow: hidden;
}

.main-card-item navigator {
  display: flex;
  flex-direction: column;
}

