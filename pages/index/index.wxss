/**校园卡**/
.xyk-balance {
  display: flex;
  align-items: flex-end;
  justify-content: center;
  font-weight: bold;
  padding: 25rpx 0 30rpx;
}

.xyk-balance-text {
  font-size: 17pt;
  line-height: 2;
  color: #999;
}

.xyk-balance-value {
  font-size: 58pt;
  line-height: 1;
  padding: 0 20rpx;
  color: #83caba;
}

.xyk-cost {
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  padding: 20rpx 0 30rpx;
  font-weight: bold;
  line-height: 200%;
}

.xyk-cost-text {
  font-size: 10pt;
  color: #999;
}

.xyk-cost-value {
  font-size: 12pt;
  font-weight: normal;
  color: #777;
  padding: 0 10rpx;
}

.xyk-cost-total {
  font-size: 16pt;
  color: #83caba;
  padding: 0 10rpx;
}

.xyk-title {
  font-size: 10pt;
}

.xyk-no-cost {
  color: #bbb;
}